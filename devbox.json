{
  "$schema": "https://raw.githubusercontent.com/jetify-com/devbox/0.16.0/.schema/devbox.schema.json",
  "packages": {
    "libiconv": "latest",
    "rustup": "latest",
    "darwin.apple_sdk.frameworks.SystemConfiguration": {
      "platforms": ["x86_64-darwin", "aarch64-darwin"]
    },
    "pkg-config": "latest",
    "openssl_3_3": "3.3.2",
    "cargo": "1.86.0"
  },
  "shell": {
    "init_hook": [
      "if [ ! -d $RUSTUP_HOME/toolchains -o ! -d $RUSTUP_HOME/toolchains/stable ]; then rustup default 1.90.0; rustup component add rust-analyzer; fi"
    ],
    "scripts": {
      "test": ["cargo test"]
    }
  }
}
